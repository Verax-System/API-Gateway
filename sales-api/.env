# --- Banco de Dados (PostgreSQL do Docker Compose) ---
# URL para o banco de dados 'sales', usando o driver asyncpg
DATABASE_URL="postgresql+asyncpg://postgres:Admin123@sales-db:5432/sales"

# --- Chave Secreta ---
# Usada para assinar tokens JWT, etc.
SECRET_KEY="UMA_CHAVE_SECRETA_PARA_A_SALES_API"

# --- Configurações de Super Usuário (Opcional, mas recomendado) ---
FIRST_SUPERUSER_EMAIL="admin@example.com"
FIRST_SUPERUSER_PASSWORD="your_strong_password"

# --- CORS ---
BACKEND_CORS_ORIGINS=["http://localhost:3000", "http://localhost:8080"]